{
    "manifest_version": 3,
    "name": "Ad Blocker Project",
    "version": "1.0.0",
    "permissions": [
        "declarativeNetRequest", "storage", "activeTab", "webNavigation", "declarativeNetRequestWithHostAccess", "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
